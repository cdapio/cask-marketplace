{
    "artifact": {
        "name": "cdap-data-pipeline",
        "version": "5.0.0-SNAPSHOT",
        "scope": "SYSTEM"
    },
    "description": "Data pipeline that copies text files from Amazon S3 to Google Cloud Storage.",
    "name": "AmazonS3ToGCSText",
    "config": {
        "resources": {
            "memoryMB": 1024,
            "virtualCores": 1
        },
        "driverResources": {
            "memoryMB": 1024,
            "virtualCores": 1
        },
        "connections": [
            {
                "from": "Amazon S3",
                "to": "Google Cloud Storage Text File"
            }
        ],
        "comments": [],
        "postActions": [],
        "properties": {},
        "processTimingEnabled": true,
        "stageLoggingEnabled": true,
        "stages": [
            {
                "name": "Amazon S3",
                "plugin": {
                    "name": "S3",
                    "type": "batchsource",
                    "label": "Amazon S3",
                    "artifact": {
                        "name": "amazon-s3-plugins",
                        "version": "1.8.1",
                        "scope": "USER"
                    },
                    "properties": {
                        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"body\",\"type\":\"string\"}]}",
                        "authenticationMethod": "Access Credentials",
                        "recursive": "false",
                        "referenceName": "AmazonS3",
                        "accessID": "${access.id}",
                        "accessKey": "${access.key}",
                        "path": "${s3.bucket.path}"
                    }
                },
                "outputSchema": [
                    {
                        "name": "etlSchemaBody",
                        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"body\",\"type\":\"string\"}]}"
                    }
                ]
            },
            {
                "name": "Google Cloud Storage Text File",
                "plugin": {
                    "name": "GCSText",
                    "type": "batchsink",
                    "label": "Google Cloud Storage Text File",
                    "artifact": {
                        "name": "google-cloud",
                        "version": "0.9.2",
                        "scope": "USER"
                    },
                    "properties": {
                        "type": "Text Comma Separated",
                        "serviceFilePath": "${service.account.file.path}",
                        "project": "${gcs.projectid}",
                        "bucket": "${gcs.bucketname}",
                        "path": "${gcs.basepath}",
                        "referenceName": "GCSText"
                    }
                },
                "outputSchema": [
                    {
                        "name": "etlSchemaBody",
                        "schema": ""
                    }
                ],
                "inputSchema": [
                    {
                        "name": "Amazon S3",
                        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"body\",\"type\":\"string\"}]}"
                    }
                ]
            }
        ],
        "schedule": "0 * * * *",
        "engine": "mapreduce",
        "numOfRecordsPreview": 100,
        "description": "Data pipeline that copies text files from Amazon S3 to Google Cloud Storage.",
        "maxConcurrentRuns": 1
    }
}